articles: env FLASK_APP=services.articles.py flask run -p 5001
tags: env FLASK_APP=services.tags.py flask run -p 5002 
comments: env FLASK_APP=services.comments.py flask run -p 5003
users: env FLASK_APP=services.users.py flask run -p 5000
init-db: env FLASK_APP=services.users.py flask init-db
init-data: env FLASK_APP=services.users.py flask init-data
reset-db: env FLASK_APP=services.users.py flask reset-db
